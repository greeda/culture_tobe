block script
    script.
        var swiper = new Swiper('.main-home-swiper .swiper-container', {
            slidesPerView: 2,
            spaceBetween: 10,
            watchOverflow: true,
            pagination: {
                el: '.swiper-pagination',
                clickable: true,
            },
            navigation: {
                nextEl: '.swiper-button-next',
                prevEl: '.swiper-button-prev',
            },
            breakpoints: {
                1025: {
                    slidesPerView: 4,
                    spaceBetween: 40,
                },
            }
        });             



                                

